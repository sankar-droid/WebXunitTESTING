@page  
@model WebXunitTESTING.Pages.EmployeeModel  

<h1>Employee Details</h1>  
<table>  
    <thead>  
        <tr>  
            <th>Id</th>  
            <td>Name</td>  
            <td>Dept No</td>  
        </tr>  
    </thead>  
    <tbody>  
        @foreach (var employee in Model.listOfEmployees)  
        {  
            <tr>  
                <td>@employee.Id</td>  
                <td>@employee.Name</td>  
                <td>@employee.DeptNo</td>  
            </tr>  
        }  
    </tbody>  
</table>
